message = "On what grounds?"

[sub_resource type="Resource" id="Resource_vm6o3"]
script = ExtResource("1_cgd38")
speaker = "Jaya"
message = "On the grounds of relevance, Your Honor. The personal relationship between my client and the person filing the complaint should not influence the judgment of this case."

[sub_resource type="Resource" id="Resource_spb1f"]
