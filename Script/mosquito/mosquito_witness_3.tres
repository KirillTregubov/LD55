message = "Mrs. Le Guin, let's try this again. Please provide a concise account of your activities on Saturday, June 9th."

[sub_resource type="Resource" id="Resource_0n5mg"]
script = ExtResource("1_oyjov")
speaker = "Ethel Le Guin"
message = "Of course, Your Honor. I woke up, found my car damaged by my daughter-in-law, and walked to the knitting store. On my way there, I passed by the location where the incident occurred. I saw a fox, but nothing seemed out of the ordinary at the time. I purchased knitting supplies and returned home without incident."

[sub_resource type="Resource" id="Resource_u1jx1"]
